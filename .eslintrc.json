{
  "extends": "next/core-web-vitals", 
  "rules": {
    "react/no-unescaped-entities": "off",
    "@next/next/no-page-custom-font": "off",
    "no-shadow": "off",
    "no-param-reassign": "off",
    "eol-last": "off",
    "import/extensions": [ 1, {
      "js": "always", "json": "always"
      }]
  },
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  // "extends": ["airbnb-base"],
  // "rules": {
  //   "no-shadow": "off",
  //   "no-param-reassign": "off",
  //   "eol-last": "off",
  //   "import/extensions": [ 1, {
  //     "js": "always", "json": "always"
  //   }]
  // },
  "ignorePatterns": [
    "dist/",
    "build/"
  ]
}
